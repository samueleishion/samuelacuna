{
  "name": "samuelacuna",
  "version": "2017.0.1",
  "description": "Samuel Acuna's portfolio site. 2017 design.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev": "npm-run-all --parallel build:dev:sass serve",
    "build:dev:sass": "node-sass --include-path ./src/sass/ --recursive ./src/sass/ -o static/stys/ --watch",
